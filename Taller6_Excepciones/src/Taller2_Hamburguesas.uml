        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_tFoFp7DTEe2Eq-A42o2mnA" name="ejecutarIniciarPedido" specification="_tEs4kbDTEe2Eq-A42o2mnA">
          <language>java</language>
          <body>// Lee nombre y direccion del cliente&#xD;
	Scanner scanner = new Scanner(System.in);&#xD;
	System.out.println(&quot;Ingrese su nombre:&quot;);&#xD;
	String nombreCliente = scanner.nextLine();&#xD;
&#xD;
	System.out.println(&quot;Ingrese su direccion:&quot;);&#xD;
	String direccionCliente = scanner.nextLine();&#xD;
&#xD;
	// IniciarPedido&#xD;
	Pedido.setNumeroPedidos(Pedido.getNumeroPedidos() + 1);&#xD;
	restaurante.iniciarPedido(nombreCliente, direccionCliente);&#xD;
&#xD;
 </body>
