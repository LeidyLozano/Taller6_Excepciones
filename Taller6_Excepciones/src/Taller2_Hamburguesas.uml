        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_tFpTxrDTEe2Eq-A42o2mnA" name="getPrecio" specification="_tEvU07DTEe2Eq-A42o2mnA">
          <ownedParameter xmi:id="_tFpTx7DTEe2Eq-A42o2mnA" direction="return">
            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
          </ownedParameter>
          <language>java</language>
          <body>double precio = 0d;&#xD;
	for (Producto item : itemsCombo) &#xD;
	{&#xD;
	    precio += item.getPrecio();&#xD;
	}&#xD;
	&#xD;
	precio = precio - (int)(precio*descuento);&#xD;
	return (int) precio;&#xD;
 </body>
        </ownedBehavior>
        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_tFpTyLDTEe2Eq-A42o2mnA" name="generarTextoFactura" specification="_tEvU1bDTEe2Eq-A42o2mnA">
          <ownedParameter xmi:id="_tFpTybDTEe2Eq-A42o2mnA" direction="return">
            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
          </ownedParameter>
          <language>java</language>
          <body>String generado = &quot;\n\nNombre del combo: &quot; + this.nombreCombo + &quot;, Items del combo:&quot;;&#xD;
	int total = 0;&#xD;
	for (Producto item : itemsCombo)&#xD;
	{&#xD;
	    total += item.getPrecio();&#xD;
	    &#xD;
	    generado += item.generarTextoFactura();&#xD;
	}&#xD;
	generado += &quot;\nTotal: &quot; + total + &quot;, Descuento: &quot; + (int) (descuento*100.0) + &quot;%, Total con descuento: &quot; + getPrecio() + &quot;, Calorias totales: &quot; + calorias + &quot;\n&quot;;&#xD;
	return generado;&#xD;
 </body>
        </ownedBehavior>
